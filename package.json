{
  "name": "stopwatch",
  "license": "UNLICENSED",
  "type": "module",
  "dependencies": {
    "@google-web-components/google-chart": "^3.0.6",
    "@polymer/iron-collapse": "^3.0.1",
    "@polymer/iron-iconset-svg": "^3.0.1",
    "@polymer/iron-localstorage": "^3.0.1",
    "@polymer/neon-animation": "^3.0.1",
    "@polymer/paper-button": "^3.0.1",
    "@polymer/paper-card": "^3.0.1",
    "@polymer/paper-icon-button": "^3.0.2",
    "@polymer/paper-input": "^3.0.2",
    "@polymer/paper-radio-button": "^3.0.1",
    "@polymer/paper-radio-group": "^3.0.1",
    "@polymer/polymer": "^3.3.0",
    "@webcomponents/webcomponentsjs": "^2.2.10",
    "gsap": "^2.0",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "browserslist": "^4.16.5",
    "cssnano": "^6.0.1",
    "eslint": "^8.53.0",
    "eslint-config-google": "^0.14.0",
    "gulp": "^4.0.2",
    "gulp-imagemin": "^9.0.0",
    "gulp-postcss": "^9.0.1",
    "postcss": "^8.4.31"
  },
  "author": "Denis Beliaev <denis@vistro.ru>",
  "browserslist": [
    "last 2 versions and > 1% in RU"
  ],
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "google"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "es6": true
    },
    "rules": {
      "brace-style": "off",
      "new-cap": [
        "error",
        {
          "capIsNewExceptions": [
            "Polymer"
          ]
        }
      ],
      "no-var": "off",
      "require-jsdoc": "off"
    },
    "globals": {
      "moment": "readonly",
      "TweenLite": "readonly",
      "Power2": "readonly"
    }
  }
}
